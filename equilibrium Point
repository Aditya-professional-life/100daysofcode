    def equilibriumPoint(self,A, N):
        # Your code here

        if N == 1:
            return 1
        
        total_sum = 0
        sum1 = 0
        
        for i in range(N):
            total_sum += A[i]
    
        for i in range(N):
            if sum1 == total_sum - sum1 - A[i]:
                return i + 1
            sum1 += A[i]
    
        return -1
    
            
